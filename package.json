{
  "name": "typecheck.macro",
  "version": "0.5.0",
  "description": "Macros for validating data against Typescript data structures",
  "scripts": {
    "format": "prettier --write \"src/**/*.{ts,js}\" --write \"tests/**/*.{js,ts}\" --write \"benchmark/**/*.{js,ts}\" --write package.json --write babel.config.json",
    "build": "babel src --out-dir dist --extensions \".ts,.js\" --ignore src/**/*.d.ts",
    "test": "npm run build && BABEL_DISABLE_CACHE=1 ava --verbose",
    "test:debug": "npm run build && ava debug --serial --verbose",
    "bench:prep": "node benchmark/generate-data.js",
    "bench": "npm run build && BABEL_DISABLE_CACHE=1 node benchmark/run.js",
    "gen-types-partial": "tsc"
  },
  "ava": {
    "require": [
      "./_register.js"
    ]
  },
  "author": "Vedant Roy",
  "license": "MIT",
  "dependencies": {
    "common-tags": "^1.8.0",
    "fast-copy": "^2.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/plugin-transform-typescript": "^7.9.4",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "@types/babel-plugin-macros": "^2.8.1",
    "@types/common-tags": "^1.8.0",
    "@types/node": "^13.13.2",
    "ajv": "^6.12.2",
    "ava": "^3.7.1",
    "babel-plugin-macros": "^2.8.0",
    "fp-ts": "^2.6.1",
    "fs-extra": "^9.0.0",
    "io-ts": "^2.2.2",
    "json-schema-faker": "^0.5.0-rcv.24",
    "lodash.isequal": "^4.5.0",
    "minimist": "^1.2.5",
    "prettier": "^2.0.5",
    "runtypes": "^4.2.0",
    "typescript": "^3.9.2",
    "zod": "^1.6.1"
  },
  "peerDependencies": {
    "babel-plugin-macros": "^2.8.0",
    "@babel/core": "^7.9.6"
  },
  "files": ["dist"],
  "main": "dist/typecheck.macro.js"
}
