{
  "name": "typecheck.macro",
  "version": "0.0.1",
  "description": "Macros for validating data against Typescript data structures",
  "scripts": {
    "format": "prettier --write \"src/*.ts\" --write \"tests/**/*.{js,ts}\" --write package.json --write babel.config.json",
    "build": "babel src --out-dir dist --extensions \".ts\"",
    "test": "npm run build && ava --verbose",
    "test:debug": "npm run build && ava debug --verbose"
  },
  "ava": {
    "require": [
      "./tests/_register.js"
    ]
  },
  "author": "Vedant Roy",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-macros": "^2.8.0",
    "common-tags": "^1.8.0"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/plugin-transform-typescript": "^7.9.4",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "@types/babel-plugin-macros": "^2.8.1",
    "@types/node": "^13.13.2",
    "ava": "^3.7.1",
    "esm": "^3.2.25",
    "prettier": "^2.0.5"
  }
}
